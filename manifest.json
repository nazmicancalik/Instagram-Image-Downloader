{
  "manifest_version": 2,

  "name": "Instagram Image Downloader",
  "description": "Save images on the instagram!",
  "version": "1.0",

  "content_scripts": [{
    "matches": [
      "http://www.instagram.com/p/*",
      "https://www.instagram.com/p/*"
    ],
    "js": ["content.js"]
  }],
  "browser_action": {
    "default_icon": "icon.png"
  }
}